<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<VBox fx:id="parent" alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
      minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" spacing="12.0" xmlns="http://javafx.com/javafx/19"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.application.controllers.DeleteEventController">
    <padding>
        <Insets bottom="18.0" left="18.0" right="18.0" top="18.0"/>
    </padding>
    <children>
        <Label text="Delete an Event">
            <font>
                <Font size="26.0"/>
            </font>
        </Label>
        <TextField fx:id="create_event_name" maxHeight="30.0" minHeight="30.0" prefHeight="30.0" promptText="Name"/>
        <DatePicker fx:id="create_event_date" maxHeight="30.0" minHeight="30.0" prefHeight="30.0" prefWidth="329.0"
                    promptText="Enter date"/>
        <HBox alignment="CENTER" spacing="12.0">
            <children>
                <Button fx:id="create_button" maxHeight="30.0" maxWidth="120.0" minHeight="30.0" minWidth="120.0"
                        mnemonicParsing="false" onAction="#onDeleteButtonListener" prefHeight="30.0" prefWidth="120.0"
                        text="Delete"/>
                <Button fx:id="cancel_button" maxHeight="30.0" maxWidth="120.0" minHeight="30.0" minWidth="120.0"
                        mnemonicParsing="false" onAction="#onCancelButtonListener" prefHeight="30.0" prefWidth="120.0"
                        text="Cancel"/>
            </children>
        </HBox>
    </children>
</VBox>
